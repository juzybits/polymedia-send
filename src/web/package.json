{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/send-web",
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist/ node_modules/ .turbo/ .wrangler/",
        "deploy-dev": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-send --branch dev",
        "deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-send --branch main",
        "dev": "vite",
        "lint": "eslint src/ *.ts --report-unused-disable-directives --max-warnings 0",
        "preview": "pnpm build && vite preview",
        "tunnel": "lt --port 1234",
        "typecheck": "tsc -b"
    },
    "dependencies": {
        "@mysten/dapp-kit": "^0.14.10",
        "@mysten/sui": "^1.2.0",
        "@polymedia/coinmeta-react": "^0.0.4",
        "@polymedia/suitcase-core": "^0.0.7",
        "@polymedia/suitcase-react": "^0.0.7",
        "@tanstack/react-query": "^5.49.2",
        "normalize.css": "^8.0.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.24.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "less": "^4.2.0",
        "localtunnel": "^2.0.2",
        "vite": "^5.3.2",
        "wrangler": "^3.62.0"
    },
    "type": "module",
    "sideEffects": false
}
