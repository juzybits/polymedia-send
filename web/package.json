{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/send-web",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://send.polymedia.app",
    "description": "Send any Sui coin simply by sharing a link",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-send"
    },
    "keywords": [],
    "scripts": {
        "build": "vite build",
        "clean": "rm -rf dist/ node_modules/ .wrangler/",
        "deploy-dev": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-send --branch dev",
        "deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-send --branch main",
        "dev": "vite",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "pnpm build && vite preview",
        "tunnel": "lt --port 1234 --subdomain poly"
    },
    "dependencies": {
        "@mysten/dapp-kit": "^0.12.7",
        "@mysten/sui.js": "^0.51.1",
        "@mysten/zksend": "^0.4.1",
        "@polymedia/suits": "^0.0.38",
        "@polymedia/webutils": "^0.0.45",
        "@tanstack/react-query": "^5.28.9",
        "normalize.css": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.3"
    },
    "devDependencies": {
        "@types/react": "^18.2.73",
        "@types/react-dom": "^18.2.23",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "less": "^4.2.0",
        "localtunnel": "^2.0.2",
        "typescript": "^5.4.3",
        "vite": "^5.2.7"
    },
    "type": "module",
    "sideEffects": false
}
